{
  "name": "@bluryar/tokenizers-js",
  "version": "0.1.57",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "dist/*.wasm"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch ."
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tokenizers-wasm": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.2.2"
  }
}
